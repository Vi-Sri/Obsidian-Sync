[run]
branch = True
source =
    obsidian-sync

[paths]
source =
    src/obsidian-sync
    **/site-packages/obsidian-sync

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug
    raise AssertionError
    raise NotImplentedError
    if 0:
    if __name__ == .__main__.:

ignore_errors = True

[html]
title = obsidian-sync Coverage Test Report
directory = docs/source/_static/coverage
